# SUPPORTED OS X VERSION        : 10.11.1
# DEPEND ON                     : timecode
#
################################################################################
### VARIABLES
################################################################################
#
# set -n # debugモード
# set -v # debugモード
# set -x # debugモード
timecodePath="$(dirname $0)/timecode"
falseCount=0
#
################################################################################
### FUNCTION
################################################################################

function testCalc() {
    # $1 --- mode -a|-s|-f|-t
    # $2 --- fps
    # $3 --- tc|fn
    # $4 --- tc
    # $5 --- answer
    if [ $# -eq 4 ]; then
        ret=$($timecodePath $1 $2 $3)
        if [ "$ret" = "$4" ]; then
            echo "  $1 $2 $3 --> $ret"
        else
            echo "X $1 $2 $3 --> $ret [$4]"
            falseCount=$((falseCount + 1))
        fi
    elif [ $# -eq 5 ]; then
        ret=$($timecodePath $1 $2 $3 $4)
        if [ "$ret" = "$5" ]; then
            echo "  $1 $2 $3 $4 --> $ret"
        else
            echo "X $1 $2 $3 $4 --> $ret [$5]"
            falseCount=$((falseCount + 1))
        fi
    fi
}

################################################################################
### MAIN
################################################################################

